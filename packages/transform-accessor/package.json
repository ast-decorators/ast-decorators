{
  "name": "@ast-decorators/transform-accessor",
  "version": "0.1.0",
  "description": "Transforms a property to an accessor with interceptors",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:remove && run-p build:js build:dts",
    "build:js": "babel src -d lib -x .ts --config-file ../../babel.config.json",
    "build:dts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:remove": "rimraf lib",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@ast-decorators/core": "^0.1.0",
    "@ast-decorators/typings": "^0.1.0",
    "@ast-decorators/utils": "^0.1.0",
    "@babel/template": "^7",
    "@babel/types": "^7"
  }
}
