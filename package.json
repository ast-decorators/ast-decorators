{
  "name": "ast-decorators",
  "private": true,
  "version": "0.1.0",
  "description": "AST Decorators core plugin",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --parallel",
    "lint": "eslint \"packages/**/*.{ts,js}\"",
    "lint-staged": "lint-staged",
    "test": "jest -c jest.config.js",
    "test:update": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ast-decorators/core.git"
  },
  "keywords": [
    "decorators",
    "ast",
    "babel"
  ],
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ast-decorators/core/issues"
  },
  "homepage": "https://github.com/ast-decorators/core#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-class-properties": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@babel/traverse": "^7.8.6",
    "@types/jest": "^25.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^13.9.0",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-poetez": "^0.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
