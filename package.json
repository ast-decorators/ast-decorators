{
  "name": "ast-decorators",
  "private": true,
  "version": "0.1.0",
  "description": "AST Decorators core plugin",
  "engines": {
    "node": ">= 12.11"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "lint": "eslint \"packages/**/*.{ts,js}\"",
    "lint-staged": "lint-staged",
    "test": "jest -c jest.config.js",
    "test:update": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ast-decorators/core.git"
  },
  "keywords": [
    "decorators",
    "ast",
    "babel"
  ],
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ast-decorators/core/issues"
  },
  "homepage": "https://github.com/ast-decorators/core#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/plugin-syntax-decorators": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/traverse": "^7.11.0",
    "@types/jest": "^26.0.9",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^14.0.27",
    "babel-jest": "^26.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-poetez": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
